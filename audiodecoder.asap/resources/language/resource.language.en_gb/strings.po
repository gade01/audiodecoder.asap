msgctxt "Addon Summary"
msgid "ASAP - Another Slight Atari Player"
msgstr ""

msgctxt "Addon Description"
msgid "ASAP (Another Slight Atari Player) plays on Kodi and converts 8-bit Atari music (*.sap, *.cmc, *.mpt, *.rmt, *.tmc, ...) on modern computers and mobile devices."
msgstr ""

#. Supported file extension description about ".sap"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30100"
msgid "A SAP file is an audio file commonly used by Atari systems that have the POKEY sound chip."
msgstr ""

#. Supported file extension description about ".cmc"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30101"
msgid "Chaos Music Composer - Atari music editor from early 1990s."
msgstr ""

#. Supported file extension description about ".cmr"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30102"
msgid "CMR (CMC \"Rzog\") - An CMC format with modified bass sounds."
msgstr ""

#. Supported file extension description about ".dmc"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30103"
msgid "DMC (CMC DoublePlay) - CMC with 6502 routine executed at double rate of the original CMC."
msgstr ""

#. Supported file extension description about ".mpt"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30104"
msgid "Music ProTracker - Atari music editor from 1990s."
msgstr ""

#. Supported file extension description about ".mpd"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30105"
msgid "MPD (MPT DoublePlay) - MPT with 6502 routine executed at double rate of the original MPT."
msgstr ""

#. Supported file extension description about ".rmt"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30106"
msgid "Raster Music Tracker - Modern Atari music editor running on Windows."
msgstr ""

#. Supported file extension description about ".tmc"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30107"
msgid "Theta Music Composer (TMC) - Atari music editor from late 1990s and played in stereo."
msgstr ""

#. Supported file extension description about ".tm8"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30108"
msgid "Theta Music Composer (TMC), supports 8 channels and differs from the similar .TMC format."
msgstr ""

#. Supported file extension description about ".tm2"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30109"
msgid "Audio file created by Theta Music Composer 2.x (TMC 2)."
msgstr ""

#. Supported file extension description about ".cms"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30110"
msgid "CMS (Stereo Double CMC)"
msgstr ""

#. Supported file extension description about ".cm3"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30111"
msgid "CMC \"3/4\" - CMC with modified pattern length."
msgstr ""

#. Supported file extension description about ".dlt"
#: audiodecoder.asap/addon.xml.in
msgctxt "#30112"
msgid "Delta Music Composer - Atari music editor from 1990s."
msgstr ""
